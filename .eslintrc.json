{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "operator-linebreak": "off"
  }
}
